{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": "server",
    "outDir": "dist",
    "lib": ["es2022"],
    "types": ["node"],
    "esModuleInterop": true,
    "strict": false,
    "skipLibCheck": true
  },
  "include": ["server/**/*.ts"]
}